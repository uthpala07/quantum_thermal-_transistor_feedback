 RowBox[{
  RowBox[{
   RowBox[{"\[Rho]Matrix", "[", "t", "]"}], "=", 
   RowBox[{"Table", "[", 
    RowBox[{
     RowBox[{
      SubscriptBox["\[Rho]", 
       RowBox[{"{", 
        RowBox[{"i", ",", "j"}], "}"}]], "[", "t", "]"}], ",", 
